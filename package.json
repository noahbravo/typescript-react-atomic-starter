{
  "name": "typescript-react-atomic-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --testMatch=\"**/*.(test).{ts,tsx}\"",
    "test:nowatch": "react-scripts test --testMatch=\"**/*.(test).{ts,tsx}\" --watchAll=false",
    "coverage": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.{js,ts,tsx}\"",
    "lint:fix": "npm run lint --fix",
    "stylelint": "stylelint \"src/**/*.{js,ts,tsx}\"",
    "format": "prettier --config .prettierrc --ignore-path .gitignore \"src/**/*.{js,ts,tsx,json}\" --write",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "stylelint",
      "eslint --ignore-path .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@radix-ui/react-polymorphic": "0.0.14",
    "@stitches/react": "^1.2.8",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.56",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.8.2",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss-html": "^1.5.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.7.1",
    "stylelint": "^14.11.0",
    "stylelint-config-recommended": "^9.0.0"
  }
}
